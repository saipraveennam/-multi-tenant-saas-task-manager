 
# Database
DB_HOST=database
DB_PORT=5432
DB_NAME=saasdb
DB_USER=postgres
DB_PASSWORD=postgres
DATABASE_URL="postgresql://postgres:postgres@database:5432/saasdb?schema=public"

# JWT Security
JWT_SECRET=your-super-secret-jwt-key-min32chars-for-multi-tenant-saas
JWT_EXPIRES_IN=24h

# Server
PORT=5000
NODE_ENV=development

# Frontend CORS (Docker service name)
FRONTEND_URL=http://frontend:3000

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX=100
